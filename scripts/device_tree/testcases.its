        /*     load = <0x84000000>; */
        /*  */
        /*     compression = "none"; */
        /*     hash@1 { */
        /*         algo = "sha1"; */
        /*     }; */
        /* }; */

    };

    configurations {
        default = "conf@1";
        conf@1 {
            description = "Run all aarch64 test cases";
            kernel = "aarch64_testcases";
            /* fdt = "fdt"; */
        };
    };
};
